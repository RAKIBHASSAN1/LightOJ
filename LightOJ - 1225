#include<bits/stdc++.h>
using namespace std;
int main()
{
long long int t,k=0;
cin>>t;
while(t--)
{
    k++;
   string n,m;
   cin>>n;
   m=n;
    reverse(n.begin(),n.end());
   if(m==n)
   {
       cout<<"Case "<<k<<": "<<"Yes"<<endl;
   }
   else
   {
        cout<<"Case "<<k<<": "<<"No"<<endl;
   }

}



}
